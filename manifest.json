{
  "name": "Scraping Linkeding",
  "description": "Extract profile Yeh!",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/*"],
      "js": ["./App/scrap.js"]
    }
  ],
  "action": {
    "default_popup": "./App/index.html",
    "default_icon": {
      "32": "./App/Image/32.png",
      "48": "./App/Image/48.png"
    }
  },
  "permissions": ["activeTab", "scripting"],
  "icons": {
    "32": "./App/Image/32.png",
    "48": "./App/Image/48.png"
  }
}
